<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Lanturnalis.Stardew.Starlight</AssemblyName>
    <RootNamespace>Lanturnalis.Stardew.Starlight</RootNamespace>
    <TargetFramework>net5.0</TargetFramework>
    <GamePath>E:\SteamLibrary\steamapps\common\Stardew Valley</GamePath>
    <DebugType>portable</DebugType>
    <DebugSymbols>True</DebugSymbols>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="SpaceCore">
      <HintPath>$(GamePath)\Mods\SpaceCore\SpaceCore.dll</HintPath>
      <Private>false</Private>
    </Reference>
  </ItemGroup>

</Project>
